    "postcss-mixins": "^6.2.0",
    "postcss-preset-env": "^5.1.0",
    "postcss-url": "^7.3.2",
    "prettier": "1.13.5",
    "react-dev-utils": "^5.0.1",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.21.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-security": "^1.3.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "scripts": {
    "build": "webpack --mode production",
    "eslint": "eslint src",
    "lint": "yarn typecheck && yarn eslint && yarn tslint",
    "precommit": "lint-staged",
    "start": "webpack-dev-server --mode development",
    "test": "jest ./src --setupTestFrameworkScriptFile=./config/jest/index.js",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "tslint": "tslint -c tslint.json '{src,test}/**/*.{ts,tsx}' --project .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "autoprefixer": "^8.6.3",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-big-calendar": "^0.19.2",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-thunk": "^2.3.0",
    "typings-for-css-modules-loader": "^1.7.0"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "{src,test}/**/*.{ts,tsx}": [
      "tslint -c tslint.json --fix",
      "git add"
    ]
  },
  "jest": {
    "testResultsProcessor": "./tests/json-reporter.js",
    "collectCoverageFrom": [
      "!src/**/*.d.ts",
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).(js|ts|tsx)?(x)",
      "<rootDir>/tests/**/?(*.)(spec|test).(js|ts|tsx)?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
